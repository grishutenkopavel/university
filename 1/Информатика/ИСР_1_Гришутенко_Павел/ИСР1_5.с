#include "stdio.h"
void main()
{
  int x;
// вывод результата различных функций
  x=-3+4*5-6; printf("1. x=%d\n", x);  // приоритеты * - + -
  x=3+4%5-6; printf("2. x=%d\n", x);  //приоритеты % + -
  x=-3+4%-6/5; printf("1. x=%d\n", x);  //приоритеты  % / - +
  x=(7+6)%5/2; printf("1. x=%d\n", x);  //приоритеты + % /
}
